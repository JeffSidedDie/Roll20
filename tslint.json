{
    "defaultSeverity": "error",
    "extends": "tslint:all",
    "jsRules": {},
    "rules": {
        "align": false,
        "linebreak-style": false,
        "max-line-length": false,
        "newline-per-chained-call": false,
        "no-implicit-dependencies": false,
        "typedef": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}