{
	"extends": "tslint:latest",
	"rules": {
		"indent": [
			true,
			"tabs"
		],
		"max-line-length": [
			false
		],
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-module",
			"check-operator",
			"check-separator",
			"check-type"
		],
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore"
		]
	}
}